Proceso Ejercicio_10
	Definir prestamo, tiempo, interes, total como real;
	Escribir "Ingrese el valor del prestamo: ";
	Leer prestamo;
	Escribir "Ingrese el tiempo: ";
	Leer tiempo;
	interes = prestamo*tiempo*0.01;
	total = prestamo + interes;
	Escribir "El monto a cancelar es de: ", total;
FinProceso
