Proceso Ejercicio_2
	Definir b,h,l1,l2,perimetro como entero;
	Definir area Como Real;
	Escribir "Ingrese el valor de la base y la altura del rectangulo";
	leer b,h;
	Escribir "Ingrese el valor de los otros dos lados del rectangulo faltantes";
	Leer l1,l2;
	area= b*h;
	perimetro = b+h+l1+l2;
	Escribir "el area del rectangulo es: ",area ,"El perimetro del rectangulo es: ", perimetro;
FinProceso