Proceso Ejecicio_5
	Definir GrCentigrados, fahrenheit, kelvin Como Real;
	Escribir "ingrese los grados centigrados";
	Leer GrCentigrados;
	fahrenheit = (GrCentigrados*(9/5))+32;
	kelvin= (GrCentigrados+273.15);
	
	Escribir "El resultado de pasar ", GrCentigrados, " a grados fahrenheit es: ", fahrenheit;
	Escribir "El resultado de pasar ", GrCentigrados, " a grados kelvin es: ", kelvin;
FinProceso
